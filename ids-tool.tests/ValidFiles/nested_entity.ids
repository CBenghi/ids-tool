<ids xmlns="http://standards.buildingsmart.org/IDS" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://standards.buildingsmart.org/IDS http://standards.buildingsmart.org/IDS/ids_09.xsd"
	 >
	<info>
		<title>The nest entity must match exactly 2/2</title>
	</info>
	<specifications>
		<specification name="The nest entity must match exactly 2/2" ifcVersion="IFC4" minOccurs="1" maxOccurs="unbounded">
			<applicability>
				<entity>
					<name>
						<simpleValue>IFCDISCRETEACCESSORY</simpleValue>
					</name>
				</entity>
			</applicability>
			<requirements>
				<partOf relation="IfcRelNests" minOccurs="1" maxOccurs="unbounded">
					<entity>
						<name>
							<simpleValue>IFCFURNITURE</simpleValue>
						</name>
					</entity>
				</partOf>
			</requirements>
		</specification>
	</specifications>
</ids>